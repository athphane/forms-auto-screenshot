    "scripts": {
        "dev": "vite",
        "build": "npm run translations && vite build",
        "translations": "node -e \"const fs = require('fs'); let src = 'resources/js/translations.js'; if (fs.existsSync('lang')) {require('child_process').execSync('php artisan lang:js ' + src + ' -s lang')}\"",
